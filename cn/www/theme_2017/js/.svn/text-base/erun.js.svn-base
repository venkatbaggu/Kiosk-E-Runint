$(document).ready(function() {
	
	//客服开关
	$(".e2-jq-service").bind("click",function(){
		$(".e2-lightbox").fadeIn(200);
		$(".e2-div-service").animate({bottom:"0"});
	});
	
	$(".e2-lightbox").bind("click",function(){
		$(".e2-div-service").animate({bottom:"-140px"});
		$(".e2-lightbox").fadeOut(200);
	});
	$(".e2-div-service-cancel").bind("click",function(){
		$(".e2-div-service").animate({bottom:"-140px"});
		$(".e2-lightbox").fadeOut(200);
	});
	
});

